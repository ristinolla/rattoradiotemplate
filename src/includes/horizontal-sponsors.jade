include variables
mixin sponsors(sponsors)
  -each sponsor in sponsors
    div.slide
      a(href="#", style="background-image: url(img/sponssit/"+sponsor+".png)") sponsor
div.wide-container
  div.horizontal-sponsors-wrap
    div.horizontal-sponsors(
      data-cycle-fx='carousel',
      data-cycle-timeout=3000,
      data-cycle-carousel-visible=4,
      data-cycle-slides='> div'
      )
      +sponsors(sponssit)   